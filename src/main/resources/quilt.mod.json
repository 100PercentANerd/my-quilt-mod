{
	"schema_version": 1,
	"quilt_loader": {
		"group": "me.vaguevocab.nerdmod",
		"id": "nerdmod",
		"version": "${version}",
		"metadata": {
			"name": "100PercentANerd's Mod",
			"description": "A short description of your mod.",
			"contributors": {
				"100PercentANerd": "Owner"
			},
			"contact": {
				"homepage": "https://github.com/",
				"issues": "https://github.com/100PercentANerd/my-quilt-mod/issues/",
				"sources": "https://github.com/100PercentANerd/my-quilt-mod/"
			},
			"icon": "assets/nerdmod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "me.vaguevocab.nerdmod.nerdmod"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.1"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "qkl",
				"versions": ">=2.1.0"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "nerdmod.mixins.json"
}
